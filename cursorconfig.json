{
  "stack": {
    "front_end": {
      "package_managers": {
        "node": {
          "name": "Node.js",
          "version": "${NODE_VERSION}",
          "url": "https://nodejs.org/docs",
          "installation": {
            "pnpm": "N/A",
            "npx": "N/A"
          }
        },
        "pnpm": {
          "name": "pnpm",
          "version": "${PNPM_VERSION}",
          "url": "https://pnpm.io/motivation",
          "installation": {
            "npm": "npm install -g pnpm@${PNPM_VERSION}",
            "npx": "N/A"
          }
        }
      },
      "framework": {
        "name": "Next.js",
        "version": "${NEXT_VERSION}",
        "url": "https://nextjs.org/docs",
        "sdk": {
          "name": "next",
          "version": "${NEXT_VERSION}",
          "url": "https://www.npmjs.com/package/next"
        },
        "installation": {
          "pnpm": "pnpm add next@${NEXT_VERSION} react react-dom",
          "npx": "npx create-next-app@latest"
        }
      },
      "gestion_etat": {
        "name": "Zustand",
        "version": "${ZUSTAND_VERSION}",
        "url": "https://docs.pmnd.rs/zustand",
        "sdk": {
          "name": "zustand",
          "version": "${ZUSTAND_VERSION}",
          "url": "https://www.npmjs.com/package/zustand"
        },
        "installation": {
          "pnpm": "pnpm add zustand@${ZUSTAND_VERSION}",
          "npx": "N/A"
        }
      },
      "authentification": {
        "name": "Clerk.dev",
        "version": "${CLERK_VERSION}",
        "url": "https://clerk.com/docs",
        "sdk": {
          "name": "@clerk/clerk-js",
          "version": "${CLERK_VERSION}",
          "url": "https://www.npmjs.com/package/@clerk/clerk-js"
        },
        "installation": {
          "pnpm": "pnpm add @clerk/clerk-js@${CLERK_VERSION}",
          "npx": "N/A"
        }
      },
      "emails": {
        "react_email": {
          "name": "React Email",
          "version": "${REACT_EMAIL_VERSION}",
          "url": "https://react.email/docs",
          "sdk": {
            "name": "react-email",
            "version": "${REACT_EMAIL_VERSION}",
            "url": "https://www.npmjs.com/package/react-email"
          },
          "installation": {
            "pnpm": "pnpm add react-email@${REACT_EMAIL_VERSION}",
            "npx": "N/A"
          }
        },
        "resend": {
          "name": "Resend",
          "version": "${RESEND_VERSION}",
          "url": "https://resend.com/docs",
          "sdk": {
            "name": "resend",
            "version": "${RESEND_VERSION}",
            "url": "https://www.npmjs.com/package/resend"
          },
          "installation": {
            "pnpm": "pnpm add resend@${RESEND_VERSION}",
            "npx": "N/A"
          }
        }
      },
      "ui": {
        "framework": {
          "name": "Tailwind CSS",
          "version": "${TAILWIND_VERSION}",
          "url": "https://tailwindcss.com/docs",
          "installation": {
            "pnpm": "pnpm add -D tailwindcss@${TAILWIND_VERSION} postcss autoprefixer",
            "npx": "npx tailwindcss init"
          }
        },
        "components": {
          "name": "shadcn/ui",
          "version": "${SHADCN_VERSION}",
          "url": "https://ui.shadcn.com/docs",
          "installation": {
            "pnpm": "pnpm dlx shadcn@latest init",
            "npx": "npx shadcn@latest init"
          }
        }
      },
      "testing": {
        "unit": {
          "name": "Vitest",
          "version": "${VITEST_VERSION}",
          "url": "https://vitest.dev/guide",
          "installation": {
            "pnpm": "pnpm add -D vitest@${VITEST_VERSION}",
            "npx": "npx vitest"
          }
        },
        "e2e": {
          "name": "Playwright",
          "version": "${PLAYWRIGHT_VERSION}",
          "url": "https://playwright.dev/docs/intro",
          "installation": {
            "pnpm": "pnpm add -D @playwright/test@${PLAYWRIGHT_VERSION}",
            "npx": "npx playwright install"
          }
        },
        "quality": {
          "name": "ESLint",
          "version": "${ESLINT_VERSION}",
          "url": "https://eslint.org/docs",
          "installation": {
            "pnpm": "pnpm add -D eslint@${ESLINT_VERSION}",
            "npx": "npx eslint --init"
          }
        }
